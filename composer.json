{
  "name": "linker/api-client",
  "description": "Linker Cloud Fulfillment Platform API Client library",
  "type": "library",
  "keywords": [
    "Api",
    "Http",
    "Client",
    "Linker Cloud Fulfillment Platform"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "guzzlehttp/guzzle": "~6.0",
    "myclabs/php-enum": "~1.2",
    "doctrine/common": "^2.5|^3.1",
    "jms/serializer": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0|~8.0"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "": "src"
    }
  },
  "autoload-dev": {
    "psr-4": { "Linker\\Tests\\": "tests/" }
  }
}
